{
  "name": "javascript-technical-test",
  "version": "1.0.0",
  "description": "Workspace para multiples problemas con Swagger",
  "workspaces": [
    "divisores-api",
    "call-center"
  ],
  "scripts": {
    "start:divisores": "cd divisores-api && node app.js",
    "start:callcenter": "cd call-center && node app.js",
    "start:all": "concurrently \"npm run start:divisores\" \"npm run start:callcenter\"",
    "install:all": "npm install"
  },
  "dependencies": {
    "express": "^5.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  }
}
